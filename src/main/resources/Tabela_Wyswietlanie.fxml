<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/17.0.12"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.TableController">
    <children>
        <ChoiceBox fx:id="tableChoiceBox" layoutX="125.0" layoutY="74.0" prefWidth="150.0">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Pracownicy" />
                    <String fx:value="Bilet" />
                    <String fx:value="Klienci" />
                    <String fx:value="Wybiegi" />
                    <String fx:value="Klatki" />
                    <String fx:value="Karmienia" />
                </FXCollections>
            </items>
        </ChoiceBox>

        <Button layoutX="300.0" layoutY="74.0" text="Odśwież" onAction="#handleRefresh" />

        <TableView fx:id="dataTableView" layoutX="100.0" layoutY="150.0"
                   prefHeight="800.0" prefWidth="1720.0">
        </TableView>
    </children>
</AnchorPane>